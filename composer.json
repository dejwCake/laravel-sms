{
    "name": "dejwcake/laravel-sms",
    "description": "A Laravel and Lumen integration for dejwcake/sms-client to enable sending SMS messages",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "sms",
        "laravel",
        "lumen"
    ],
    "authors": [
        {
            "name": "David BÄ›hal",
            "email": "behal.david@gmail.com",
            "role": "Fork Developer"
        },
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com",
            "role": "Original Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "dejwcake/sms-client": "^3.0",
        "guzzlehttp/guzzle": "^7.10"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.8",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "DejwCake\\LaravelSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DejwCake\\LaravelSms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Sms": "DejwCake\\LaravelSms\\Facades\\Sms"
            },
            "providers": [
                "LaravelSmsProvider"
            ]
        }
    }
}
