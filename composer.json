{
    "name": "dejwcake/laravel-sms",
    "description": "A Laravel and Lumen integration for dejwcake/sms-client to enable sending SMS messages",
    "type": "library",
    "keywords": ["sms","laravel","lumen"],
    "require": {
        "php": "^8.1",
        "dejwcake/sms-client": "dev-v2.0-rc"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6",
        "phpunit/phpunit": "^9.5.21",
        "mockery/mockery": "^1.5.0",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.3.0",
        "aws/aws-sdk-php": "3.*",
        "psy/psysh": "^0.11.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com",
            "role": "Original Developer"
        },
        {
            "name": "David BÄ›hal",
            "email": "behal.david@gmail.com",
            "role": "Fork Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelSMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelSMS\\LaravelSMSProvider"
            ],
            "aliases": {
                "SMS": "Matthewbdaly\\LaravelSMS\\Facade"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
